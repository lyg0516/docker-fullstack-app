{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "leeyeonggeun/docker-frontend",
            "hostname" : "frontend",
            "essential": false,
            "memory" : 128
        },
        {
            "name": "backend",
            "image": "leeyeonggeun/docker-backend",
            "hostname": "backend",
            "essential": false,
            "memory" : 128
        },
        {
            "name": "nginx",
            "image": "leeyeonggeun/docker-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPorst": 80,
                    "containerPort": 80
                }
            ],
            "links": ["frontend", "backend"],
            "memory" :128
        }
    ]
}